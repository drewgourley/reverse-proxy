{
  "name": "reverse-proxy",
  "version": "1.0.0",
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "tsc -p tsconfig.client.json",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules app.ts"
  },
  "dependencies": {
    "@aws-sdk/client-route-53": "^3.515.0",
    "adm-zip": "^0.5.16",
    "ajv": "^8.17.1",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "connect-redis": "^9.0.0",
    "dotenv": "^16.4.3",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.4",
    "express-subdomain": "^1.0.6",
    "gamedig": "^5.3.2",
    "got": "^11.8.6",
    "http-proxy-middleware": "^2.0.6",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "redis": "^5.10.0",
    "serve-index": "^1.9.1",
    "sharp": "^0.34.5",
    "to-ico": "^1.0.1",
    "wake_on_lan": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "@types/node": "^20.4.1",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.5",
    "@types/body-parser": "^1.19.2",
    "@types/multer": "^1.4.7"
  },
  "overrides": {
    "fast-xml-parser": ">5.3.3"
  }
}
